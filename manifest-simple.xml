<?xml version="1.0" encoding="UTF-8"?>
<!--
  SIMPLIFIED MANIFEST FOR QUICK TESTING
  This version works without icon files - easier for first-time setup

  Replace YOUR_USERNAME with your GitHub username before using!
-->
<OfficeApp
  xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:type="TaskPaneApp">

  <Id>c8e4b3a2-1d5f-4e6a-9b2c-7f8d0e1a3b5c</Id>
  <Version>1.0.0</Version>
  <ProviderName>Yichang Wu</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Weekly Plan"/>
  <Description DefaultValue="Weekly planning and timetable management with habits tracking"/>

  <!--
    ⚠️ REPLACE YOUR_USERNAME below with your actual GitHub username!

    For LOCAL testing, use: https://localhost:3000/taskpane.html
  -->
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000/taskpane.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>

</OfficeApp>
